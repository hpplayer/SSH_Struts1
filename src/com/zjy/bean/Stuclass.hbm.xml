<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zjy.bean.Stuclass" table="stu_class">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="stuID" column="stuID" type="java.lang.Integer" />
		<property name="classID" column="classID" type="java.lang.Integer" />
		<property name="grade" column="grade" type="java.lang.Integer" />
		<property name="score" column="score" type="java.lang.Integer" />
	</class>
</hibernate-mapping>